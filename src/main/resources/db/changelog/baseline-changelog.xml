<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
                   xmlns:pro="http://www.liquibase.org/xml/ns/pro"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		           http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd
		           http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
		           http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd">

    <changeSet author="AR" id="1666858884712-1">
        <createTable tableName="car">
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="city" type="VARCHAR(255)"/>
            <column name="date" type="VARCHAR(255)"/>
            <column name="vehicle_type" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
    <changeSet author="AR" id="1666858884712-2">
        <createTable tableName="hibernate_sequence">
            <column name="next_val" type="BIGINT"/>
        </createTable>
    </changeSet>
    <changeSet author="AR" id="1666858884712-3">
        <createTable tableName="tariff">
            <column name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="amount" type="VARCHAR(255)"/>
            <column name="city" type="VARCHAR(255)"/>
            <column name="time_interval" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>